                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>WITH CTE1 AS
(
SELECT TBLTRAANSDETAILS.AccountId, TBL_Accountes.AccountDes, SUM(TBLTRAANSDETAILS.Madeen) AS Madeen, SUM(TBLTRAANSDETAILS.Daien) AS Daien 
, CASE WHEN TBL_Accountes.AccNatueID = 1 THEN SUM(TBLTRAANSDETAILS.Madeen) - SUM(TBLTRAANSDETAILS.Daien) ELSE SUM(TBLTRAANSDETAILS.Daien) - SUM(TBLTRAANSDETAILS.Madeen) END AS Balance
, 0 AS [raseedmadeen], 0 AS [raseeddaen], 0 AS raseedBalance
FROM TBLTRAANSDETAILS INNER JOIN TBLTRANSACTION ON TBLTRAANSDETAILS.TRANSID = TBLTRANSACTION.TRANSID 
INNER JOIN TBL_Accountes ON TBLTRAANSDETAILS.AccountId = TBL_Accountes.AccountId
WHERE TBLTRANSACTION.YearID = @YearID
GROUP BY TBLTRAANSDETAILS.AccountId, TBL_Accountes.AccountDes, TBL_Accountes.AccNatueID
UNION
SELECT tblraseed.[AccountId], TBL_Accountes.AccountDes, 0, 0, 0
, SUM([raseedmadeen]) AS [raseedmadeen], SUM([raseeddaen]) AS [raseeddaen]
, CASE WHEN TBL_Accountes.AccNatueID = 1 THEN SUM(tblraseed.[raseedmadeen]) - SUM(tblraseed.raseeddaen) ELSE SUM(tblraseed.raseeddaen) - SUM(tblraseed.[raseedmadeen]) END AS raseedBalance

FROM dbo.tblraseed INNER JOIN TBL_Accountes ON [tblraseed].AccountId = TBL_Accountes.AccountId
WHERE tblraseed.YearID = @YearID
GROUP BY tblraseed.[AccountId], TBL_Accountes.AccountDes, TBL_Accountes.AccNatueID
)
SELECT AccountId, AccountDes, SUM(Madeen) AS Madeen, SUM(Daien) AS Daien, SUM(Balance) AS Balance
, SUM([raseedmadeen]) AS [raseedmadeen], SUM([raseeddaen]) AS [raseeddaen], SUM(raseedBalance) AS raseedBalance
, SUM(Madeen) + SUM(raseedmadeen) AS Final_Madeen, SUM(Daien) + SUM(raseeddaen) AS Final_Daien
, SUM(Balance) + SUM(raseedBalance) AS Final_Balance
FROM CTE1 GROUP BY AccountId, AccountDes</CommandText>
                    <Parameters>
